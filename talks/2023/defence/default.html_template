        <script src="./js/simulation.js"></script>
        <script>
			let sim_div = document.getElementById('sim_div');
			if( !(sim_div === null) ) {
				let sim_emt_p5 = new p5(sim_emt, 'sim_div');
				sim_emt_p5.keyTyped = function() {
					if( sim_emt_p5.key === 'e' ) {
						sim_emt_p5.pause();
					}
					if( sim_emt_p5.key === 'd' ) {
						sim_emt_p5.init();
					}
				}
			}
		</script>
